<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="book" namespace="/book" extends="struts-default">
		<default-class-ref class="com.action.BookAction" />
		<global-results>
			<result name="input" >/success.jsp</result>
			<result name="success" >/success.jsp</result>
		</global-results>
		<action name="delete" method="delete">
			<result name="input" >/success.jsp</result>
			<result name="success" >/success.jsp</result>
		</action>
		<action name="deletes" method="deletes">
			<result name="input" >/success.jsp</result>
			<result name="success" >/success.jsp</result>
		</action>
		<action name="query" method="query">
			<result>/book.jsp</result>
		</action>
		<action name="add" method="add">
			<result name="input" >/success.jsp</result>
			<result name="success" >/success.jsp</result>
		</action>


	</package>
</struts>
